algoritmo "somar_num"
// Função : Somar dois números
// Autor : Barbara Rocha
// Data : 20/08/2018
var
   num1, num2, resultado: real

inicio
   escreval ("Informe valor 1:")
   leia(num1)
   escreval ("Informe valor 2:")
   leia(num2)
   resultado <- num1 * num2
   escreva ("O resultado da multiplicação entre ", num1, " e", num2, " È: ", resultado)
fimalgoritmo
